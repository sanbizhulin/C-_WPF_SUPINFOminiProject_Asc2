            </Button.RenderTransform>
        </Button>
        <Button Content="Validation" Name="Validation" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top" Width="280"/>
        <Button Content="QuitMenu" HorizontalAlignment="Left" Margin="10,119,0,0" VerticalAlignment="Top" Width="280"/>
    </Grid>
</Page>
